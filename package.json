    "jszip": "^3.10.1",
    "marked": "^16.4.1",
    "next": "16.0.3",
    "qs": "^6.14.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sanitize-html": "^2.17.0",
    "server-only": "^0.0.1",
    "sharp": "0.32.6",
    "swiper": "12.0.3",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
