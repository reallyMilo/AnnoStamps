{
  "name": "annostamps",
  "version": "0.0.0",
  "private": true,
  "description": "Community site for sharing stamps in Anno game series.",
  "type": "module",
  "scripts": {
    "analyze-build": "ANALZYE=true pnpm build",
    "build": "next build",
    "cypress": "cypress open",
    "db-seed": "prisma db seed",
    "dev": "next dev --turbopack",
    "lint": "eslint .",
    "migrate": "pnpx prisma migrate dev --skip-generate && pnpx prisma generate --sql",
    "prepare": "husky",
    "reset-db": "pnpx prisma migrate reset",
    "start": "next start",
    "studio": "prisma studio",
    "test": "vitest run",
    "test:e2e": "cypress run --browser chrome",
    "test:watch": "vitest",
    "vitest": "vitest --ui"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.666.0",
    "@aws-sdk/client-s3": "^3.620.1",
    "@aws-sdk/s3-request-presigner": "^3.620.1",
